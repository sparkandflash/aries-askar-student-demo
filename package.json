{
    "name": "student-demo",
    "version": "0.1.0",
    "private": true,
    "workspaces": [
        "client",
        "server"
    ],
    "scripts": {
        "check-types": "yarn workspaces run tsc --noEmit -p tsconfig.json",
        "prettier": "prettier '**/*.+(js|json|ts|md|yml|yaml)'",
        "lint": "eslint --ignore-path .gitignore .",
        "format": "yarn prettier --write",
        "client": "yarn workspace client start",
        "server":"yarn workspace server start",
        "test-server":"yarn workspace server test",
        "next-client":"yarn workspace next-client dev", 
        "test-client":"yarn workspace test-client start",
        "build-server": "yarn workspace server build"
    },
    "devDependencies": {
        "@aries-framework/core": "^0.2.3",
        "@typescript-eslint/eslint-plugin": "^5.6.0",
        "@typescript-eslint/parser": "^5.38.1",
        "@typescript-eslint/visitor-keys": "^5.10.2",
        "concurrently": "^7.0.0",
        "eslint": "^8.23.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^3.5.1",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-prettier": "^4.0.0",
        "prettier": "^2.6.1",
        "rxjs": "^7.5.7",
        "typescript": "~4.8.4"
    },
    "dependencies": {
        "@types/node": "^20.9.4",
        "axios": "^1.6.2",
        "ngrok": "^5.0.0-beta.2",
        "react-redux": "^8.1.3",
        "redux": "^4.2.1",
        "routing-controllers": "^0.10.4",
        "ts-node": "^10.9.1",
        "typedi": "^0.10.0"
    }
}
